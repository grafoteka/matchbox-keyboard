        </key>
        <key obey-caps="true">
            <default display="ν" />
            <shifted display="Ν" />
        </key>
        <key obey-caps="true">
            <default display="μ" />
            <shifted display="Μ" />
        </key>
        <key width="3000">
            <default display=" " action="space" />
        </key>
    </row>
</fragment>
